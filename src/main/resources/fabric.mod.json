{
  "schemaVersion": 1,
  "id": "addon-template",
  "version": "${version}",
  "name": "Addon Template",
  "description": "addon made solely from suggestions on meteors github",
  "authors": [
    "tympanic"
  ],
  "contact": {
    "repo": "https://github.com/tympanicblock61/meteor-suggestions-addon"
  },
  "icon": "assets/tympanic/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "com.tympanic.suggestion.Addon"
    ]
  },
  "mixins": [
    "suggestion.mixins.json"
  ],
  "custom": {
    "meteor-client:color": "0,255,255"
  },
  "depends": {
    "java": ">=17",
    "minecraft": ">=${mc_version}",
    "meteor-client": "*"
  }
}
